<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_account_nav_graph"
    app:startDestination="@+id/profileFragment">

    <fragment
        android:id="@+id/profileFragment"
        android:name="cl.cencosud.blogapp.home.ui.myaccount.MyAccountFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_my_account">
        <action
            android:id="@+id/got_to_profile"
            app:destination="@+id/dynamicFeatureProfile" />

    </fragment>

    <include-dynamic
        android:id="@id/dynamicFeatureProfile"
        android:name="cl.cencosud.blogapp.login"
        app:graphResName="profile_nav_graph"
        app:moduleName="profile" />

</navigation>