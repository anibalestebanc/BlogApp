<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="cl.cencosud.blogapp.login.ui.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login">

        <action
            android:id="@+id/got_to_home"
            app:destination="@+id/dynamicFeatureHome" />

    </fragment>

    <include-dynamic
        android:id="@id/dynamicFeatureHome"
        android:name="cl.cencosud.blogapp.home"
        app:graphResName="home_nav_graph"
        app:moduleName="home" />

</navigation>